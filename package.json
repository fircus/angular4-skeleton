{
  "name": "angular_4_skeleton",
  "version": "4.4.5",
  "main": "dist/server.js",
  "scripts": {
    "start": "npm run build && npm run server",
    "build": "webpack",
    "build:aot": "webpack --env.aot --env.client & webpack --env.aot --env.server",
    "build:prod:client": "webpack --env.aot --env.client -p",
    "build:prod:server": "webpack --env.aot --env.server -p",
    "prebuild": "yarn run clean",
    "clean": "rimraf dist",
    "server": "nodemon dist/server.js",
    "server:prod": "node dist/server.js",
    "watch": "webpack --watch",
    "compile:scss": "node scss-config.js",
    "clean:css": "rimraf src/**/*.css",
    "test": "karma start"
  },
  "engines": {
    "node": ">=8.9.1",
    "npm": ">=5.6.0"
  },
  "dependencies": {
    "@angular/animations": "^4.4.5",
    "@angular/common": "^4.4.5",
    "@angular/compiler": "^4.4.5",
    "@angular/compiler-cli": "^4.4.5",
    "@angular/core": "^4.4.5",
    "@angular/forms": "^4.4.5",
    "@angular/http": "^4.4.5",
    "@angular/platform-browser": "^4.4.5",
    "@angular/platform-browser-dynamic": "^4.4.5",
    "@angular/platform-server": "^4.4.5",
    "@angular/router": "^4.4.5",
    "@nguniversal/express-engine": "^1.0.0-beta.2",
    "@ngx-translate/core": "^7.1.0",
    "@ngx-translate/http-loader": "^1.0.2",
    "angular2-router-loader": "^0.3.5",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.2.3",
    "body-parser": "^1.17.2",
    "bootstrap": "^4.0.0-alpha.6",
    "compression": "^1.7.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "rxjs": "^5.4.3",
    "sass-loader": "^6.0.6",
    "webpack-node-externals": "^1.6.0",
    "zone.js": "0.8.12",
    "@ngtools/webpack": "^1.6.2",
    "@types/express": "^4.11.0",
    "copy-webpack-plugin": "^4.0.1",
    "nodemon": "^1.12.1",
    "raw-loader": "^0.5.1",
    "script-ext-html-webpack-plugin": "^1.8.5",
    "typescript": "^2.6.2",
    "webpack": "3.5.3",
    "webpack-merge": "^4.1.0"
  },
  "devDependencies": {
    "@types/karma-jasmine": "0.0.29",
    "jasmine-core": "^2.8.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.1",
    "karma-spec-reporter": "0.0.32"
  }
}
